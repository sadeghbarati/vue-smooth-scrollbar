{
  "name": "vue-smooth-scrollbar",
  "version": "0.0.1",
  "packageManager": "pnpm@7.2.1",
  "description": "Starter template to build component library for vue.js 2-3",
  "author": "Sadegh Barati",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sadeghbarati/vue-smooth-scrollbar"
  },
  "bugs": "https://github.com/sadeghbarati/vue-smooth-scrollbar/issues",
  "keywords": [
    "starter",
    "boilerplate",
    "vue",
    "vue3",
    "component",
    "ui",
    "frontend"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    }
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.13.2"
  },
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:coverage": "rimraf coverage",
    "dev": "vite --mode dev",
    "build": "pnpm clean:dist && vite build --mode prod && vue-tsc --emitDeclarationOnly",
    "preview": "vite preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "pnpm clean:coverage && vitest --coverage --mode test",
    "commit": "cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "@vue/composition-api": "^1.4.9",
    "smooth-scrollbar": "8.7.4",
    "vue": "^2.0.0 || >=3.0.0"
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "gitHooks": {
    "commit-msg": "npx --no-install commitlint --edit $1",
    "pre-commit": "pnpm lint",
    "prepare-commit-msg": "exec < /dev/tty && np --hook || true"
  },
  "dependencies": {
    "vue-demi": "^0.13.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "latest",
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "@types/node": "^17.0.42",
    "@vitejs/plugin-vue": "latest",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.17.0",
    "jsdom": "^19.0.0",
    "rimraf": "^3.0.2",
    "smooth-scrollbar": "^8.7.4",
    "typescript": "^4.7.3",
    "vite": "^2.9.12",
    "vitest": "^0.14.2",
    "vue": "^3.2.37",
    "vue-tsc": "^0.37.5",
    "yorkie": "^2.0.0"
  }
}
